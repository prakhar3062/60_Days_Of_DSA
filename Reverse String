//Recursion
class Solution {
public:
    void reverseStringI(vector<char>& s,int i){
       int n=s.size();
        if(i==n/2)
            return;
         swap(s[i],s[n-i-1]);
         reverseStringI(s,i+1);
    }
    void reverseString(vector<char>& s) {
        reverseStringI(s,0);
       
    }
};
//Loop
class Solution {
public:
    void reverseString(vector<char>& s) {
        int n=s.size();
        for(int i=0;i<n/2;i++){
            swap(s[i],s[n-i-1]);
        }
    }
};
